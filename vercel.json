{
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    },
    "api/simple.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/simple-(.*)",
      "destination": "/api/simple.js"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 